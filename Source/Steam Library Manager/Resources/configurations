#--------------------------------------------------------------------------
version: 2.0
root:
  name: "Might & Magic Heroes VI"
  sort_string: "Heroes 6"
  background_image: 2cae962b44a2e89411a941a29d53dbf2.jpg
  thumb_image: ee92b261c5543e345e0e1ecd70d30b71.jpg
  logo_image: eff5b67aac96a8b6e440e4d516885bbb.png
  dialog_image: df4097065ab2784938696b6d590f72f8.png
  background_color: 000000
  start_game:
    online:
      overlay_supported: yes
      legacy_ticket_enabled: yes
      after_game_report_enabled: yes
      executables:
        - path: 
            relative: Might & Magic Heroes VI.exe
          working_directory:
            register: HKEY_LOCAL_MACHINE\SOFTWARE\Ubisoft\Might & Magic Heroes VI\InstallPath      
          evil_updater:
            path: 
              relative: gu.exe
            working_directory:
              register: HKEY_LOCAL_MACHINE\SOFTWARE\Ubisoft\Might & Magic Heroes VI\InstallPath
    offline:
      after_game_report_enabled: yes
      executables:
        - path: 
            relative: Might & Magic Heroes VI.exe
          arguments: -playoffline
          working_directory:
            register: HKEY_LOCAL_MACHINE\SOFTWARE\Ubisoft\Might & Magic Heroes VI\InstallPath
    legacy_offline:
      executable:
        path:
          relative: Might & Magic Heroes VI.exe
        arguments: -playoffline
        working_directory:
          register: HKEY_LOCAL_MACHINE\SOFTWARE\Ubisoft\Might & Magic Heroes VI\InstallPath
  cloud_saves: yes
  game_streaming_enabled: yes
  legacy_friends_nicks_enabled: yes
  uplay:
    game_code: MMH6
  homepage_url: http://www.mmh6.com
  help_url: http://support.ubi.com
  forum_url: http://www.mightandmagic.com/forums
  facebook_url: https://www.facebook.com/MightandMagic
  after_game_report_ad: yes
